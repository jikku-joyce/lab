<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ServiceDAO" table="service">
   		<id name="serviceId" column="service_id" type="integer">
     		<generator class="assigned" />
  		</id>
   		<property name="serviceName" column="service_name" type="string"/>
   		<property name="serviceCharge" column="service_charge" type="double"/>
  	</class>
</hibernate-mapping>